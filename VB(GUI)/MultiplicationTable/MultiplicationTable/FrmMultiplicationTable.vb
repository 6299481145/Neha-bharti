Public Class FrmMultiplicationTable
    Private Sub Btn2_Click(sender As Object, e As EventArgs) Handles Btn2.Click
        Dim n, p As Integer
        n = 2
        TxtMultiplicationTable.Clear()

        For i As Integer = 1 To 10
            p = n * i
            TxtMultiplicationTable.Text = TxtMultiplicationTable.Text & n & "×" & i & "=" & p & vbCrLf
        Next
        TxtMessage.Text = "After laerning the table of " & n & ", learn the table of " & n + 1 & "."
    End Sub

